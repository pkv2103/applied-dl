n","        <script src=\"/static/components/requirejs/require.js\"></script>\n","        <script>\n","          requirejs.config({\n","            paths: {\n","              base: '/static/base',\n","              plotly: 'https://cdn.plot.ly/plotly-latest.min.js?noext',\n","            },\n","          });\n","        </script>\n","        '''))"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"9g6QZfMDH_Tp","colab_type":"code","colab":{}},"source":["def plotly_df(df, title='', annotations=None):\n","    \"\"\"Visualize all the dataframe columns as line plots.\"\"\"\n","    common_kw = dict(x=df.index, mode='lines+markers')\n","    xaxis = dict(title='Time Steps',type='date', rangeslider=dict(visible=True))\n","    \n","    data = [go.Scatter(y=df[c], name=c,  **common_kw) for c in df.columns]\n","    layout = dict(title=title, showlegend=True, annotations=annotations, xaxis=xaxis, )\n","    fig = dict(data=data, layout=layout)\n","    iplot(fig, show_link=False)"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"kzgCGjiiH_UQ","colab_type":"code","colab":{}},"source":["def create_timeseries_dataset(data, n_in=1, n_out=1, dropnan=True):\n","    \"\"\"\n","    Frame a time series as a supervised learning dataset.\n","    Arguments:\n","        data: Sequence of observations as a list or NumPy array.\n","        n_in: Number of lag observations as input (X).\n","        n_out: Number of observations as output (y).\n","        dropnan: Boolean whether or not to drop rows with NaN values.\n","    Returns:\n","        Pandas DataFrame of series framed for supervised learning.\n","    \"\"\"\n","    n_vars = 1 if type(data) is list else data.shape[1]\n","    df = pd.DataFrame(data)\n","    cols, names = list(), list()\n","\n","    # input sequence (t-n, ... t-1)\n","    for i in range(n_in, 0, -1):\n","        cols.append(df.shift(i))\n","        names += [('var_lag%d(t-%d)' % (j+1, i)) for j in range(n_vars)]\n","\n","    # forecast sequence (t, t+1, ... t+n)\n","    for i in range(0, n_out):\n","        cols.append(df.shift(-i))\n","        if i == 0:\n","            names += [('var_roll%d(t)' % (j+1)) for j in range(n_vars)]\n","        else:\n","            names += [('var_roll%d(t+%d)' % (j+1, i)) for j in range(n_vars)]\n","    # put it all together\n","    agg = pd.concat(cols, axis=1)\n","    agg.columns = names\n","    # drop rows with NaN values\n","    if dropnan:\n","        agg.dropna(inplace=True)\n","    return agg"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"n2G2BADbkGBZ","colab_type":"code","colab":{}},"source":["def timeseries_train_test_split(X, test_size):\n","    \"\"\"\n","        Perform train-test split with respect to time series structure\n","    \"\"\"\n","    \n","    # get the index after which test set starts\n","    test_index = int(len(X)*(1-test_size))\n","    \n","    train = X[:test_index]\n","    test = X[test_index:]\n","    \n","    return train, test"],"execution_count":0,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"pzzEtf06jx-P","colab_type":"text"},"source":["**Read the Dataset & add index**"]},{"cell_type":"code","metadata":{"id":"ntRJIM7BH_T1","colab_type":"code","colab":{}},"source":["df_raw = pd.read_csv(filepath_or_buffer=data_filename, header=0, names=None, usecols=['date', 'sale_amt'], parse_dates=[0])"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"HTl2R1MkoaIj","colab_type":"code","colab":{}},"source":["df_raw.index = df_raw.date\n","df_raw.drop(['date'], inplace=True, axis=1)"],"execution_count":0,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"Zj4kTwHeo53g","colab_type":"text"},"source":["**Plot the dataset**"]},{"cell_type":"code","metadata":{"id":"Z5S_vPssoWoG","colab_type":"code","outputId":"e953e78a-c423-4df5-d597-748155b4a1af","executionInfo":{"status":"ok","timestamp":1558240961478,"user_tz":240,"elapsed":705,"user":{"displayName":"Phani Valasa","photoUrl"